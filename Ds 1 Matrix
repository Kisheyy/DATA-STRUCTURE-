 #include<stdio.h>
int main()
{
	int a[10][10],b[10][10],mul[10][10],i,j,n,m,k;
	printf("Enter the no. of rows: ");
	scanf("%d",&n);
	printf("Enter the no. of columns: ");
	scanf("%d",&m);
	for(i=0;i<n;i++)
		for(j=0;j<n;j++)
			scanf("%d",&a[i][j]);
	for(i=0;i<n;i++)
		for(j=0;j<n;j++)
			scanf("%d",&b[i][j]);
	for(i=0;i<n;i++)
		for(j=0;j<n;j++)
		mul[i][j]=k;
		for(k=0;k<n;k++)
			mul[i][j]+=a[i][k]*b[k][j];
	printf("Multiplication Matrix= ");
	for(i=0;i<n;i++)
		for(j=0;j<n;j++)
			printf("%d",mul[i][j]);
return 0;	
}
